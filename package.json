{
  "name": "vitepress-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vitepress dev interview-docs",
    "build": "vitepress build interview-docs",
    "serve": "vitepress serve interview-docs",
    "deploy": "npm run build && gh-pages -d interview-docs/.vitepress/dist",
    "dist": "vitepress build interview-docs && npx rimraf docs && npx cpr interview-docs/.vitepress/dist docs -o"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "vitepress": "^1.6.4",
    "vue": "^3.5.26",
    "rimraf": "^6.0.1",
    "cpr": "^3.0.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
